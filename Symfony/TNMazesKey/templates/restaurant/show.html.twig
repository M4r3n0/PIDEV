						</div>

						<div class="col-md-6">
							<div class="details">


								{% block body %}
									<center>
										<h1>Restaurant
											{{ restaurant.nom }}</h1>

										<table class="table">
											<tbody>
												<tr>
													<th>Id</th>
													<td>{{ restaurant.id }}</td>
												</tr>
												<tr>
													<th>Nom</th>
													<td>{{ restaurant.nom }}</td>
												</tr>
												<tr>
													<th>Adresse</th>
													<td>{{ restaurant.adresse }}</td>
												</tr>
												<tr>
													<th>Menu</th>
													<td>{{ restaurant.menu }}</td>
												</tr>
												<tr>
													<th>Programme</th>
													<td>{{ restaurant.prog }}</td>
												</tr>
												<tr>
													<th>Telephone</th>
													<td>{{ restaurant.telephone }}</td>
												</tr>
												<tr>
													<th>Nombre de places
													</th>
													<td>{{ restaurant.nbrep }}</td>
												</tr>
												<tr>
													<th>Note</th>
													<td>{{ restaurant.note }}</td>
												</tr>
											</tbody>
										</table>
										<button>
											<a href="{{ path('app_restaurant_index') }}">back to list</a>
										</button>
										<button>
											<a href="{{ path('app_restaurant_edit', {'id': restaurant.id}) }}">edit</a>
										</button>
										{{ include('restaurant/_delete_form.html.twig') }}
									</center>
								{% endblock %}

							</div>
						</div>

					</div>
				</div>

			</section>
